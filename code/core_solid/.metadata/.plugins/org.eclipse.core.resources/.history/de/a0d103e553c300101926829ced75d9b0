package com.demo.basics;
//"seprate what varies"

class Bird {
	void sound() {
		System.out.println("bird sound");
	}
	void swim() {
		System.out.println("bird swim");
	}
	void fly() {
		System.out.println("bird fly");
	}
}

class Eagle extends Bird implements Flyable {

	@Override
	public void fly() {
		System.out.println("flying high...");
	}
	
}


