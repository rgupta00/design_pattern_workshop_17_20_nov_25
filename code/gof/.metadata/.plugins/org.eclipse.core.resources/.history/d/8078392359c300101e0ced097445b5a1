	public void insertTohole(String str) {
		System.out.println("roundpeg insertTohole(): " + str);
	}
}
class SquarePeg {
	public void insert(String str) {
		System.out.println("SquarePeg insert(): " + str);
	}
}
class PegAdaptor extends SquarePeg {

}

public class Demo {

